{
  "name": "goodschema",
  "version": "0.0.0",
  "description": "a good router",
  "type": "module",
  "bin": "out/program.js",
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "out/*",
    "src/*"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "clean": "rm -rf out",
    "test": "npm run spec-all",
    "lint": "eslint \"src/**/*.ts\"",
    "spec-all": "node ./out/**/*.spec.js",
    "coverage": "c8 --reporter lcov --reporter cobertura npm test",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:goodschema/goodschema-ts.git"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goodschema/goodschema-ts/issues"
  },
  "keywords": [
    "good",
    "json",
    "schema"
  ],
  "homepage": "https://ts.goodschema.org",
  "devDependencies": {
    "@types/node": "^18.15.7",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "c8": "^7.13.0",
    "eslint": "^8.36.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-security": "^1.7.1",
    "typedoc": "^0.23.28"
  },
  "dependencies": {
    "@types/yargs": "^17.0.22",
    "camelcase": "^7.0.1",
    "crc": "^4.3.2",
    "tslib": "^2.5.0",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "yargs": "^17.7.1"
  }
}
